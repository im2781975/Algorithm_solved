#include <iostream>
using namespace std;
 
int max_edges(int V, int E) 
{ 
      return V * (V - 1) / 2 - E; 
}
 
int main()
{
    int V, E;
    // The number of vertices in the DAG
    V = 6;
    // The number of edges in the DAG
    E = 6;
      // Final output
    cout << "Maximum number of edges that can be added to maintain DAG is " << max_edges(V, E);
    return 0;
}
