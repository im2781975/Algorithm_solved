#include <algorithm>
#include <iostream>
#include <sstream>
#include <string>
 
std::string makeReverse(std::string str)
{
    std::reverse(str.begin(), str.end());
    std::istringstream iss(str);
    std::string word;
    std::string reverse;
    while (iss >> word) {
        reverse = word + " " + reverse;
    }
    return reverse;
}
 
int main()
{
    std::string str = "Geeks for Geeks";
    std::cout << makeReverse(str) << std::endl;
    return 0;
}
